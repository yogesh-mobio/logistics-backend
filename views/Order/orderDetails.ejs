<%- contentFor('HeaderCss') %> <%- contentFor('breadcrumb') %>
<ul class="list-inline menu-left mb-0">
  <li class="list-inline-item">
    <button type="button" class="button-menu-mobile open-left waves-effect">
      <i class="ion-navicon"></i>
    </button>
  </li>
  <li class="hide-phone list-inline-item app-search">
    <h3 class="page-title">Order Details</h3>
  </li>
</ul>

<%- contentFor('body') %>
<div class="page-content-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="card m-b-20">
          <div class="card-body">
            <h4 class="mt-0 header-title">
              See all the details of a particular order
            </h4>
            <br /><br />
            <% include ../Partials/errorMessages %>
            <form method="POST" class="form-horizontal">
              <div class="form-group row">
                <label for="orderId" class="col-sm-2 col-form-label"
                  >Order ID</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderId"
                    name="OrderId"
                    value="<%= order.orderData.order_id %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="clientName" class="col-sm-2 col-form-label"
                  >Client</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="clientName"
                    name="client"
                    value="<%= order.customer.customerData.first_name %> <%=
                  order.customer.customerData.last_name %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="orderPrice" class="col-sm-2 col-form-label"
                  >Price</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderPrice"
                    name="price"
                    value="<%= order.orderData.price %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="orderPaymentMode" class="col-sm-2 col-form-label"
                  >Payment Mode</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderPaymentMode"
                    name="paymentMode"
                    value="<%= order.orderData.payment_mode %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="orderStatus" class="col-sm-2 col-form-label"
                  >Status</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderStatus"
                    name="status"
                    value="<%= order.orderData.status %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="orderCreatedAt" class="col-sm-2 col-form-label"
                  >Order Date</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderCreatedAt"
                    name="createdAt"
                    value="<%= order.orderData.created_at.toDate() %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="orderSource" class="col-sm-2 col-form-label"
                  >Source</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderSource"
                    name="source"
                    value="<%= order.orderData.pickup_location.flat_name %>"
                    readonly
                  />
