                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="orderDestination"
                    name="destination"
                    value="<%= order.orderData.drop_location.flat_name %>"
                    readonly
                  />
                  <br />
                  <div
                    class="input-group bootstrap-touchspin bootstrap-touchspin-injected"
                  >
