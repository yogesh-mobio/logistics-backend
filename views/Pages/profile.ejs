<%- contentFor('HeaderCss') %> <%- contentFor('breadcrumb') %>
<ul class="list-inline menu-left mb-0">
  <li class="list-inline-item">
    <button type="button" class="button-menu-mobile open-left waves-effect">
      <i class="ion-navicon"></i>
    </button>
  </li>
  <li class="hide-phone list-inline-item app-search">
    <h3 class="page-title">
      <%= admin.userAdmin.first_name %>&nbsp;<%= admin.userAdmin.last_name %>
    </h3>
  </li>
</ul>

<%- contentFor('body') %>
<div class="page-content-wrapper">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        <div class="card m-b-20">
          <div class="card-body">
            <h4 class="mt-0 header-title">Admin Profile</h4>
            <br />
            <% include ../Partials/errorMessages %>
            <form class="form-horizontal">
              <div class="form-group row">
                <label for="adminFirstName" class="col-sm-2 col-form-label"
                  >First Name</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="adminFirstName"
                    name="firstname"
                    placeholder="Enter your first name"
                    value="<%= admin.userAdmin.first_name %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="adminLastName" class="col-sm-2 col-form-label"
                  >Last Name</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="adminLastName"
                    name="lastname"
                    placeholder="Enter your last name"
                    value="<%= admin.userAdmin.last_name %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="adminPhoneNumber" class="col-sm-2 col-form-label"
                  >Phone Number</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="adminPhoneNumber"
                    name="phone"
                    placeholder="Enter your Phone Number"
                    value="<%= admin.userAdmin.phone_number %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="adminEmail" class="col-sm-2 col-form-label"
                  >Email</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="email"
                    id="adminEmail"
                    name="email"
                    placeholder="Enter your Email ID"
                    value="<%= admin.userAdmin.email %>"
                    readonly
                  />
                </div>
              </div>

              <div class="form-group row">
                <label for="adminStatus" class="col-sm-2 col-form-label"
                  >Status</label
                >
                <div class="col-sm-10">
                  <input
                    class="form-control"
                    type="text"
                    id="adminStatus"
                    name="status"
                    placeholder="Enter your Status"
                    value="<%= admin.status %>"
                    readonly
                  />
                </div>
              </div>

              <button
                type="button"
                class="btn btn-primary"
                onclick="location.href='/update-profile'"
              >
                Edit your profile
              </button>

              <!-- <button
                type="button"
                class="btn btn-danger"
                onclick="location.href='/dashboard'"
              >
                Cancel
              </button> -->
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%- contentFor('FooterJs') %> <%- contentFor('BottomJs') %>
